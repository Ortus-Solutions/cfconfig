	  },
	  "net.sourceforge.jtds.jdbc.Driver": {
		"label": "jTDS (MSSQL,Sybase)",
		"id": "jtds",
		"bundleName": "jtds",
		"bundleVersion": "1.3.1"
	  }
	},
	"cacheClasses": [
	  {
		"class": "org.lucee.extension.cache.eh.EHCache",
		"bundleName": "ehcache.extension",
		"bundleVersion": "2.10.0.36"
	  }
	]
  }